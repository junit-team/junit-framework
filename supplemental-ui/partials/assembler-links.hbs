{{#unless (eq page.componentVersion.prerelease '-SNAPSHOT')}}
    {{#with (resolvePage page.relativeSrcPath model=false)}}
        {{#with ./assembler.pdf}}
        <div class="pdf-link">
            <a href="{{{relativize ./file.pub.url}}}{{{./fragment}}}">PDF</a>
        </div>
        {{/with}}
    {{/with}}
{{/unless}}
{{#with (resolvePage page.relativeSrcPath model=false)}}
    {{#with ./assembler.html}}
        <div class="html-link">
            <a href="{{{relativize ./file.pub.url}}}{{{./fragment}}}">Single page</a>
        </div>
    {{/with}}
{{/with}}
